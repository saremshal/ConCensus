                                        <script>
                                            const queryString = window.location.search;
                                            const urlParams = new URLSearchParams(queryString);
                                            var name = urlParams.get("search[name]");

                                            var data = new Array();
                                            var year = new Array();
                                            var temp = new Array();

                                            //Gets the Year and Data for a given Municipality
                                            <% @municipality_social_data.each do |municipality_social_datum| %>                                          
                                                if ("<%= municipality_social_datum.name %>" === name ) {
                                                    temp.push([<%= municipality_social_datum.year %>, <%= municipality_social_datum.households %>]);
                                                }
                                            <% end %>                                             

                                            //Sorts Temp Array based on Year
                                            temp.sort(sortFunction);
                                            function sortFunction(a, b) {
                                                if (a[0] === b[0]) {
                                                    return 0;
                                                } else {
                                                    return (a[0] < b[0]) ? -1 : 1;
                                                }
                                            }

                                            //Push Sorted Data into Data Array
                                            for (var i = 0; i < temp.length; i++) {
                                                year.push(temp[i][0]);
                                                data.push(temp[i][1]);
                                            }
                                            
                                            //Create Line Graph
                                            var ctx = document.getElementById('household_graph').getContext('2d');
                                            var households = new Chart(ctx, {
                                                type: 'line',
                                                data: {
                                                    labels: year,
                                                    datasets: [{
                                                        label: "Total Households in " + name,
                                                        data: data
                                                    }]
                                                }
                                            });
                                            
                                        </script>
