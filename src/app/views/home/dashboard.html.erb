                                            }
                                            
                                            //Create Line Graph
                                            var ctx = document.getElementById('highschool_grad_graph').getContext('2d');
                                            var households = new Chart(ctx, {
                                                type: 'line',
                                                data: {
                                                    labels: year,
                                                    datasets: [{
                                                        label: "Total People that are Highschool Graduates in " + name,
                                                        data: data
                                                    }]
                                                }
                                            });
                                            
                                        </script>
                                    </div>
                                </div>
